{
  "name": "ton-audit-platform",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "dev": "pnpm --filter @ton-audit/web dev",
    "dev:web": "pnpm --filter @ton-audit/web dev",
    "dev:worker": "pnpm --filter @ton-audit/worker dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test",
    "db:generate": "pnpm --filter @ton-audit/web db:generate",
    "db:migrate": "pnpm --filter @ton-audit/web db:migrate"
  }
}